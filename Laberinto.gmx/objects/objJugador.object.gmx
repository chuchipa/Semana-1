    {
    if (!place_meeting(x,y-velocidad,objWall))&amp;&amp; (!place_meeting(x-velocidad,y,objPuertaPadre))&amp;&amp; (!place_meeting(x-velocidad,y,objPuerta ))
    {y = y-velocidad;}
