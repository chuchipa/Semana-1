    {
    if (!place_meeting(x+velocidad,y,objWall))&amp;&amp; (!place_meeting(x-velocidad,y,objPuertaPadre))&amp;&amp; (!place_meeting(x-velocidad,y,objPuerta ))
    {x = x+velocidad;}
    }
    
